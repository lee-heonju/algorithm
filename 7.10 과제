#2
def print_ma():
    a=int(input())
    b=int(input())
    c=int(input())
    if a>b:
        if a>c:
           print(a)
        else:
           print(c)
    if b>c:
       print(b)
    else:
        print(c)
        
#3
def avg_numbers(*args):
    result = 0
    for i in args:
        result += i
    return result / len(args)
print(avg_numbers(1, 2)) #>>> 1.5
print(avg_numbers(1,2,3,4,5)) #>>> 3.0

